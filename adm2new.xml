<?xml version="1.0" encoding="UTF-8"?>
<nta>
  <declaration>chan grant; chan patent; chan pub; chan coin;</declaration>
  <template>
    <name>Adm2</name>
    <declaration>clock x; clock y;</declaration>
    <location id="L20" x="0" y="0">
      <name>L20</name>
      <label kind="invariant">true</label>
    </location>
    <location id="L21" x="0" y="0">
      <name>L21</name>
      <label kind="invariant">y&lt;=2</label>
    </location>
    <location id="L22" x="0" y="0">
      <name>L22</name>
      <label kind="invariant">x&lt;=2</label>
    </location>
    <location id="L23" x="0" y="0">
      <name>L23</name>
      <label kind="invariant">(x&lt;=2 &amp;&amp; y&lt;=2)</label>
    </location>
    <init ref="L20" />
    <transition>
      <source ref="L20" />
      <target ref="L21" />
      <label kind="synchronisation">pub?</label>
      <label kind="guard">true</label>
      <label kind="assignment">y = 0</label>
    </transition>
    <transition controllable="false">
      <source ref="L21" />
      <target ref="L20" />
      <label kind="synchronisation">patent!</label>
      <label kind="guard">true</label>
      <label kind="assignment" />
    </transition>
    <transition>
      <source ref="L21" />
      <target ref="L21" />
      <label kind="synchronisation">pub?</label>
      <label kind="guard">(y&lt;=2 &amp;&amp; y-x&lt;=2)</label>
      <label kind="assignment" />
    </transition>
    <transition>
      <source ref="L21" />
      <target ref="L23" />
      <label kind="synchronisation">grant?</label>
      <label kind="guard">(y&lt;=2 &amp;&amp; y-x&lt;=2)</label>
      <label kind="assignment">x = 0</label>
    </transition>
    <transition controllable="false">
      <source ref="L23" />
      <target ref="L21" />
      <label kind="synchronisation">coin!</label>
      <label kind="guard">(y&lt;=2 &amp;&amp; y-x&lt;=2)</label>
      <label kind="assignment" />
    </transition>
    <transition>
      <source ref="L23" />
      <target ref="L23" />
      <label kind="synchronisation">pub?</label>
      <label kind="guard">(x&lt;=2 &amp;&amp; y&lt;=2 &amp;&amp; y-x&lt;=2 &amp;&amp; x-y&lt;=2)</label>
      <label kind="assignment" />
    </transition>
    <transition>
      <source ref="L23" />
      <target ref="L23" />
      <label kind="synchronisation">grant?</label>
      <label kind="guard">(x&lt;=2 &amp;&amp; y&lt;=2 &amp;&amp; y-x&lt;=2 &amp;&amp; x-y&lt;=2)</label>
      <label kind="assignment" />
    </transition>
    <transition controllable="false">
      <source ref="L23" />
      <target ref="L22" />
      <label kind="synchronisation">patent!</label>
      <label kind="guard">(x&lt;=2 &amp;&amp; x-y&lt;=2)</label>
      <label kind="assignment" />
    </transition>
    <transition>
      <source ref="L22" />
      <target ref="L23" />
      <label kind="synchronisation">pub?</label>
      <label kind="guard">(x&lt;=2 &amp;&amp; x-y&lt;=2)</label>
      <label kind="assignment">y = 0</label>
    </transition>
    <transition>
      <source ref="L22" />
      <target ref="L22" />
      <label kind="synchronisation">grant?</label>
      <label kind="guard">(x&lt;=2 &amp;&amp; x-y&lt;=2)</label>
      <label kind="assignment" />
    </transition>
    <transition controllable="false">
      <source ref="L22" />
      <target ref="L20" />
      <label kind="synchronisation">coin!</label>
      <label kind="guard">true</label>
      <label kind="assignment" />
    </transition>
    <transition>
      <source ref="L20" />
      <target ref="L22" />
      <label kind="synchronisation">grant?</label>
      <label kind="guard">true</label>
      <label kind="assignment">x = 0</label>
    </transition>
  </template>
  <system>system Adm2;</system>
</nta>
